{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth(?:\\/(.*?))?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/500\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/auth\\/confirm-email\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/auth\\/forgot-password\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/auth\\/reset-password\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/auth\\/sign-in\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/auth\\/sign-up\\/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^\\/$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "/.*",
			"dest": "_middleware",
			"status": 404
		}
	]
}