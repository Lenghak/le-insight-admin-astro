---
import { setURLStore } from "@/common/stores/url-store";
import DashboardLayout from "@dashboard/layouts/dashboard-layout.astro";
import UsersTable from "@dashboard/subs/users/composites/users-table";
import UsersTabs from "@dashboard/subs/users/composites/users-tabs";

export const prerender = false;
const url = Astro.url;

setURLStore(url);
---

<DashboardLayout dashboardTitle="Users">
  <!-- Tabs -->
  <UsersTabs
    client:only="react"
    transition:persist
  />

  <!-- Tables -->
  <UsersTable client:only="react" />
</DashboardLayout>
